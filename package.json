{
  "name": "todomvc-stimulus",
  "version": "0.42.0",
  "description": "TodoMVC implementation using Stimulus",
  "main": "index.ts",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs rollup.config.js",
    "start": "concurrently --kill-others --names build,web-dev-server \"npm run build -- --watch\" \"web-dev-server --config web-dev-server.config.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .js --report-unused-disable-directives --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gowda/todomvc-stimulus.git"
  },
  "keywords": [
    "todomvc",
    "stimulus"
  ],
  "author": "Basavanagowda <basavanagowda@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gowda/todomvc-stimulus/issues"
  },
  "homepage": "https://github.com/gowda/todomvc-stimulus#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint": "^8.56.10",
    "@web/dev-server": "^0.4.5",
    "@web/dev-server-rollup": "^0.6.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rollup": "^4.18.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "todomvc-app-css": "^2.4.3",
    "todomvc-common": "^1.0.5"
  }
}
